{
  "name": "namor",
  "version": "0.1.5",
  "description": "a manly, url-friendly name generator",
  "keywords": [
    "manly",
    "man",
    "womanly",
    "name",
    "subdomain",
    "generator",
    "random",
    "url"
  ],
  "author": "Jason Maurer <j@maur.co>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsonmaur/namor.git"
  },
  "files": [
    "assets/",
    "build/",
    "lib/",
    "data.json",
    "README.md",
    "LICENSE"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib .nyc_output coverage",
    "lint": "eslint src tests",
    "pretest": "npm run clean && npm run lint",
    "test": "nyc ava tests -a -i babel-register",
    "posttest": "nyc report -r=lcov",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "build": "npm run clean && babel src -d lib -s --no-comments",
    "prebuild-browser": "rm -rf build && mkdir build",
    "build-browser": "browserify -g uglifyify -t babelify src/umd -o build/namor.min.js",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build-browser && npm run build"
  },
  "devDependencies": {
    "ava": "0.13.0",
    "babel-cli": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-3": "6.5.0",
    "babel-register": "6.7.2",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "coveralls": "2.11.8",
    "eslint": "2.4.0",
    "eslint-config-zab": "0.1.1",
    "nyc": "6.1.1",
    "uglifyify": "3.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  },
  "eslintConfig": {
    "extends": "zab"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "failFast": true
  },
  "nyc": {
    "include": [
      "src"
    ]
  }
}
